<div class=container>
  <div class="col-md-4">
    <div class="buffer-both">
      <%= link_to 'Создать пользователя', new_user_path, class: 'btn btn-success' %>
    </div>
    <%= render partial: 'users/user', collection: @users %>
  </div>
  <div class="col-md-8">
    <div class="buffer-both">
      <%= link_to 'Создать событие', new_event_path, class: 'btn btn-success' %>
    </div>
    <%= render partial: 'events/event', collection: @events %>
  </div>
</div>

